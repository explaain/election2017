  <body class='desktop'>
    <%- include partials/preloaders.ejs %>
    <div id="app"></div>
    <script type="text/javascript">
      var Model,
          Routes;
      var Embed = <%= embed %>;
      var SiteBrand = getParameterByName('brand') || "<%= brand %>";
      var Standalone = <%= standalone %>;
      var QuizPage = <%= quiz %>;
      if (Standalone || Embed) {
        var StepName = "<%= step %>";
        var PhraseName = "<%= phrase %>";
      }

      function getParameterByName(name, url) {
        if (!url) url = window.location.href;
        name = name.replace(/[\[\]]/g, "\\$&");
        var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
            results = regex.exec(url);
        if (!results) return null;
        if (!results[2]) return '';
        return decodeURIComponent(results[2].replace(/\+/g, " "));
      }
    </script>
    <script src="/compiled.js?v=<%=NOCACHE%>" type="text/javascript"></script>
    <script type="text/javascript">
      $('button').click(function(e) {
        console.log("Button clicked", e);
        Rollbar.debug("Button clicked", e);
      })
      $('a').click(function(e) {
        console.log("Link clicked", e);
        Rollbar.debug("Link clicked", e);
      })
      $('.startQuiz').click(function(e) {
        Rollbar.debug(".startQuiz clicked");
      })
      $('.quizCountrySelector').click(function(e) {
        Rollbar.debug(".quizCountrySelector clicked");
      })
      Rollbar.debug("Testing Testing");
    </script>
    <%- include partials/addthis.ejs %>
